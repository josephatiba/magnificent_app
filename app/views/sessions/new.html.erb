<div class="container">
  <div class="row">
   <div class="col-xs-6 col-xs-offset-3">


  <div>
	<h1>Log In</h1>
  </div>

	<%= form_for :login do |f| %>


	<div>
		<%= f.label :email %>
		<%= f.text_field :email %>
	</div>

	<div>
		<%= f.label :password %>
		<%= f.password_field :password %>
	</div>

	<div>
		<%= f.submit 'Log In!' %>
	</div>


	<% end %>
  </div>
 </div>
</div>




